{
  "extends": [
    "plugin:jsdoc/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "reportUnusedDisableDirectives": true,
  "rules": {
    "semi": ["error", "always"],
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "asyncArrow": "always",
      "named": "never"
    }]
  }
}
